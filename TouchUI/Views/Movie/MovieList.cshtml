@model List<BioscoopB3Web.Domain.Entities.HallMovie>

@{
    ViewBag.Title = "MovieList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center">@ViewBag.MovieList</h2>

<div class="container-fluid col-md-12">
    <div class="row ">
        @foreach (var m in Model)
        {
            @Html.Partial("HallMovie", m)
        }

    </div>
    <br />
</div>


